    "range-admin": "^0.1.4",
    "uuid": "^9.0.1"
  },
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
